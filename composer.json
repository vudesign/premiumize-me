{
  "name": "vu/premiumize-me",
  "version": "1.0.0",
  "type": "contao-bundle",
  "description": "Contao 4 Premiumize.me API",
  "license": "MIT",
  "authors": [
    {
      "name": "Manh Cuong Vu"	  
    }
  ],
  "require": {
    "php": ">=7.1",
    "contao/core-bundle": "^4"   
  },
  "autoload": {
    "psr-4": {
      "Vu\\PremiumizeMe\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/assets",
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Vu\\PremiumizeMe\\ContaoManager\\Plugin"
  }
}
